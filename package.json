{
  "name": "tauri-docs",
  "type": "module",
  "private": true,
  "license": "MIT",
  "version": "0.0.1",
  "scripts": {
    "dev:setup:tauri": "yarn --cwd packages/tauri/tooling/api",
    "dev:setup:plugins-workspace": "pnpm --prefix packages/plugins-workspace install",
    "dev:setup": "pnpm dev:setup:tauri && pnpm dev:setup:plugins-workspace",
    "dev": "astro dev",
    "build:reference": "",
    "build:astro": "astro build",
    "build:i18n": "pnpm --filter docs-i18n-tracker run build",
    "build": "pnpm dev:setup && pnpm build:astro && pnpm build:i18n",
    "preview": "astro preview"
  },
  "dependencies": {
    "@astrojs/markdown-remark": "^2.2.1",
    "@astrojs/starlight": "^0.6.0",
    "@types/json-schema": "^7.0.12",
    "astro": "^2.9.6",
    "rehype-autolink-headings": "^6.1.1",
    "sharp": "^0.32.4"
  },
  "pnpm": {
    "patchedDependencies": {
      "typedoc-plugin-markdown@4.0.0-next.17": "patches/typedoc-plugin-markdown@4.0.0-next.17.patch"
    }
  }
}
