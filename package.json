    "preview": "astro preview"
  },
  "dependencies": {
    "@astrojs/starlight": "^0.5.6",
    "astro": "^2.9.3",
    "sharp": "^0.32.4",
    "starlight-typedoc": "^0.3.0",
    "typedoc": "^0.24.8",
    "typedoc-plugin-markdown": "4.0.0-next.17",
    "typedoc-plugin-mdn-links": "^3.0.3"
  }
